/******************************  QPushButton  ******************************/
QPushButton{
		border-radius: 6px;
		border: 1px outset rgb(111, 156, 207);
		background:qlineargradient(spread:pad,x1:0,y1:0,x2:0,y2:1,stop:0 rgba(182, 225, 229, 255),stop:1 rgba(30,76,79,255));
		padding:4px;
}

QPushButton:enabled {
		color: rgb(236, 245, 252);
}

QPushButton:enabled:hover{
		border-style: outset;
		background:qlineargradient(spread:pad,x1:0,y1:0,x2:0,y2:1,stop:0 rgba(100,160,220,128),stop:1 rgba(100,160,220,255));
		color: black;
}
QPushButton:enabled:pressed{
		background: rgb(0,78,161);
		border-style:inset;
		color: white;
}
QPushButton:enabled:checked{
		background: rgb(0,78,161);
		border-style:inset;
		color: white;
}

QPushButton:!enabled {
		background: rgb(180,180,180);
		color: white;
}

QPushButton[behavior="exit"]:enabled:hover{
		background: rgb(232,17,35);
}

QPushButton[behavior="apply"]{
		background:qlineargradient(spread:pad,x1:0,y1:0,x2:0,y2:1,stop:0 rgba(203,240,144,128),stop:1 rgba(120,174,18,128));
		color:black;
}

QPushButton[behavior="apply"]:enabled:hover{
		background:qlineargradient(spread:pad,x1:0,y1:0,x2:0,y2:1,stop:0 rgba(120,174,18,255),stop:1 rgba(203,240,144,255));
		color:blue;
}

QPushButton[behavior="cancel"]{
		background:qlineargradient(spread:pad,x1:0,y1:0,x2:0,y2:1,stop:0 rgba(240,169,145,128),stop:1 rgba(174,58,19,128));
		color:black;
}

QPushButton[behavior="cancel"]:enabled:hover{
		background:qlineargradient(spread:pad,x1:0,y1:0,x2:0,y2:1,stop:0 rgba(174,58,19,255),stop:1 rgba(240,169,145,255));
		color:blue;
}

QPushButton[behavior="stop"]{
		background:qlineargradient(spread:pad,x1:0,y1:0,x2:0,y2:1,stop:0 rgba(240,169,145,128),stop:1 rgba(174,58,19,128));
		border: 1px outset rgb(255, 127, 39);
		font-weight:bold;
		color:black;
		text-align:bottom;
		padding-bottom: 13px;
		background-image:url(./styles/Image/btnlaunch.png);
		background-repeat:no-repeat;
		background-attachment:fixed;
		background-position:center;
}

QPushButton[behavior="stop"]:enabled:hover{
		background:qlineargradient(spread:pad,x1:0,y1:0,x2:0,y2:1,stop:0 rgba(174,58,19,255),stop:1 rgba(240,169,145,255));
		text-align:bottom;
		padding-bottom: 13px;
		background-image:url(./styles/Image/btnlaunch.png);
		background-repeat:no-repeat;
		background-attachment:fixed;
		background-position:center;
}

QPushButton[behavior="stop"]:enabled:pressed{
		text-align:bottom;
		padding-bottom: 13px;
		background-image:url(./styles/Image/btnlaunch.png);
		background-repeat:no-repeat;
		background-attachment:fixed;
		background-position:center;
}

QPushButton[behavior="stop"]:!enabled{
		text-align:bottom;
		padding-bottom: 13px;
		background-image:url(./styles/Image/btnforbid.png);
		background-repeat:no-repeat;
		background-attachment:fixed;
		background-position:center;
		color: white;
}

QPushButton[behavior="launch"],
QPushButton[behavior="launch"]:enabled:hover,
QPushButton[behavior="launch"]:enabled:pressed{
		text-align:bottom;
		padding-bottom: 13px;
		background-image:url(./styles/Image/btnlaunch.png);
		background-repeat:no-repeat;
		background-attachment:fixed;
		background-position:center;
}

QPushButton[behavior="launch"]:!enabled{
		text-align:bottom;
		padding-bottom: 13px;
		background-image:url(./styles/Image/btnforbid.png);
		background-repeat:no-repeat;
		background-attachment:fixed;
		background-position:center;
		color: white;
}

