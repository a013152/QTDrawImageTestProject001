/******************************  QHeaderView  ******************************/
QHeaderView{
        background: rgba(182, 225, 229, 255);
				min-height: 26px;
				min-width: 26px;
				text-align:center;
}
QHeaderView::section:vertical{
				border: none;
				background: rgba(225, 243, 244, 255);
				border-right: 1px solid rgba(94, 190, 198, 255);
				border-bottom: 1px solid rgba(216, 216, 216, 255);
}
/******************************  QScrollBar-水平  ******************************/
QScrollBar:horizontal {
        height: 20px;
        background: transparent;
        margin-top: 3px;
        margin-bottom: 3px;
}
QScrollBar::handle:horizontal {
        height: 20px;
        min-width: 30px;
        background: rgb(170, 200, 230);
        margin-left: 15px;
        margin-right: 15px;
}
QScrollBar::handle:horizontal:hover {
        background: rgb(165, 195, 225);
}
QScrollBar::sub-line:horizontal {
        width: 15px;
        background: transparent;
        image: url(./Style/Image/arrowleft.png);
        subcontrol-position: left;
}
QScrollBar::add-line:horizontal {
        width: 15px;
        background: transparent;
        image: url(./Style/Image/arrowright.png);
        subcontrol-position: right;
}
QScrollBar::sub-line:horizontal:hover {
        background: rgb(170, 200, 230);
        image: url(./Style/Image/arrowleft-on.png);
}
QScrollBar::add-line:horizontal:hover {
        background: rgb(170, 200, 230);
        image: url(./Style/Image/arrowright-on.png);
}
QScrollBar::add-page:horizontal,QScrollBar::sub-page:horizontal {
        background: transparent;
}

/******************************  QScrollBar-垂直  ******************************/
QScrollBar:vertical {
        width: 20px;
        background: transparent;
        margin-left: 3px;
        margin-right: 3px;
}
QScrollBar::handle:vertical {
        width: 20px;
        min-height: 30px;
        background: rgb(170, 200, 230);
        margin-top: 15px;
        margin-bottom: 15px;
}
QScrollBar::handle:vertical:hover {
        background: rgb(165, 195, 225);
}
QScrollBar::sub-line:vertical {
        height: 15px;
        background: transparent;
        image: url(./Style/Image/arrowup.png);
        subcontrol-position: top;
}
QScrollBar::add-line:vertical {
        height: 15px;
        background: transparent;
        image: url(./Style/Image/arrowdown.png);
        subcontrol-position: bottom;
}
QScrollBar::sub-line:vertical:hover {
        background: rgb(170, 200, 230);
        image: url(./Style/Image/arrowup-on.png);
}
QScrollBar::add-line:vertical:hover {
        background: rgb(170, 200, 230);
        image: url(./Style/Image/arrowdown-on.png);
}
QScrollBar::add-page:vertical, QScrollBar::sub-page:vertical {
        background: transparent;
}

QScrollBar#verticalScrollBar:vertical {
        margin-top: 30px;
}

/******************************  QComboBox  ******************************/
QComboBox 
{
		border-radius: 3px;
		border: 1px solid rgb(111, 156, 207);
		background: rgb(239, 239, 239);
		padding-left: 6px;
		font-size: 18px;
		font-weight: bold;
		height: 26px;
}
QComboBox:enabled 
{
		color: rgb(64, 64, 64);
}
QComboBox:!enabled 
{
		color: rgb(80, 80, 80);
}
QComboBox:enabled:hover, 
QComboBox:enabled:focus 
{
		color: rgb(48, 48, 48);
		background: rgb(255, 255, 255);
}

QComboBox::drop-down 
{
		subcontrol-origin: padding;
		subcontrol-position: top right;
		width: 22px;
		border-left-width: 1px;
		border-left-color: darkgray;
		border-left-style: solid;
		border-top-right-radius: 3px;
		border-bottom-right-radius: 3px;
}

QComboBox::drop-down:hover 
{
		background: rgba(255, 255, 255, 30);
}
QComboBox::down-arrow 
{
		image: url(./styles/Image/arrowdown.png) center center no-repeat;
}

QComboBox::down-arrow:hover 
{
		image: url(./styles/Image/arrowdown.png) center center no-repeat;
}

QComboBox::down-arrow:on 
{
		image: url(./styles/Image/arrowdown.png) center center no-repeat;
}

QComboBox QAbstractItemView 
{
		border: 1px solid rgb(193, 203, 215);
		background: rgb(239, 239, 239);
		outline: none;
}
QComboBox QAbstractItemView::item
{
		outline: none;
		padding-left: 6px;
		font-size: 18px;
		font-weight: bold;
		color: rgb(96, 96, 96);
		height: 26px;
}
QComboBox QAbstractItemView::item:selected 
{
		background: rgb(100,160,220);
		color: rgb(255,255,255);
		padding-left: 8px;
}

