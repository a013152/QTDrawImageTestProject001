/******************************  QPushButton  ******************************/
QPushButton{
		border-radius: 3px;
		border: 0px outset rgb(111, 156, 207);
		background:qlineargradient(spread:pad,x1:0,y1:0,x2:0,y2:1,stop:0 rgba(49, 79, 115, 255),stop:1 rgba(49, 79, 115, 255));
		padding:4px;
}

QPushButton:enabled {
		color: rgb(236, 245, 252);
}

QPushButton:enabled:hover{
		border-style: outset;
		background:qlineargradient(spread:pad,x1:0,y1:0,x2:0,y2:1,stop:0 rgba(60,104,157,255),stop:1 rgba(60,104,157,255));
		color: white;
}
QPushButton:enabled:pressed{
		background: rgb(53,68,87);
		border-style:inset;
		color: white;
}
QPushButton:enabled:checked{
		background: rgb(0,78,161);
		border-style:inset;
		color: white;
}

QPushButton:!enabled {
		background: rgb(202,202,202);
		color: rgb(64,64,75);
}

QPushButton[behavior="exit"]{		
		min-width: 23px;
		min-height: 23px;
		border: 1px outset rgb(111, 156, 207);
}
QPushButton[behavior="exit"]:enabled:hover{
		background: rgb(220,20,60);
		border: 0px outset rgb(0, 0, 0);
} 

QPushButton[behavior="stop"]{
		border: 1px outset rgba(255, 255, 255, 0);
		background: rgba(255, 255, 255, 0);		
		font-weight:bold;
		color:black;
		text-align:bottom;
		padding-bottom: -20px;
		background-image:url(./styles/Image/btn_stop_c602.png);
		background-repeat:no-repeat;
		background-attachment:fixed;
		background-position:center;
}

