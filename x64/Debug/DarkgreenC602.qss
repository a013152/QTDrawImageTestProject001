QPushButton[behavior="stop"]:enabled:pressed{
		background: rgba(255,255,255,0);
		background-image:url(./styles/Image/btn_stop_c602.png);
		border: 0px outset rgba(255, 255, 255, 0);
}


QPushButton[behavior="stop"]:!enabled{
		background: rgba(255, 255, 255, 0);
		text-align:bottom;
