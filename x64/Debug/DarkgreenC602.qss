/******************************  QHeaderView  ******************************/
QHeaderView{
        background: rgba(48, 79, 115, 255);
				min-height: 26px;
				min-width: 26px;
				text-align:center;
}
QHeaderView::section:vertical{
				border: none;
				background: rgba(225, 243, 244, 0);
				border-right: 1px solid rgba(94, 190, 198, 255);
				border-bottom: 1px solid rgba(216, 216, 216, 255);
}
QHeaderView::section{
	color:white;
	background:rgb(48, 79, 115);
}
/******************************  QScrollBar-水平  ******************************/
QScrollBar:horizontal {
        height: 20px;
        background: transparent;
        margin-top: 3px;
        margin-bottom: 3px;
}
QScrollBar::handle:horizontal {
        height: 20px;
        min-width: 30px;
        background: rgb(170, 200, 230);
        margin-left: 15px;
        margin-right: 15px;
}
QScrollBar::handle:horizontal:hover {
        background: rgb(165, 195, 225);
}
QScrollBar::sub-line:horizontal {
        width: 15px;
        background: transparent;
        image: url(./Style/Image/arrowleft.png);
        subcontrol-position: left;
}
QScrollBar::add-line:horizontal {
        width: 15px;
        background: transparent;
        image: url(./Style/Image/arrowright.png);
        subcontrol-position: right;
}
QScrollBar::sub-line:horizontal:hover {
        background: rgb(170, 200, 230);
