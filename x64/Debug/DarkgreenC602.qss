

/******************************  MainForm  ******************************/
QFrame#mainbackground
{
		background: black;
		border: 3px ridge rgb(30, 81, 85);
		border-radius: 1px;
		color: rgb(255, 255, 255);
}

QWidget#maintitle
{
		background:qlineargradient(spread:pad,x1:0,y1:0,x2:0,y2:1,stop:0 rgba(48,79,115,255),stop:1 rgba(48,79,115,255));
		border-radius: 1px;
}

QLabel#maincaption
{
		padding: 10px;
		color: rgb(255, 255, 255);
		font-size: 26px;
		font-family: 宋体;
}

QLabel#version
{
		color: rgba(30,76,79,127);
		font-size: 14px;
		font-family: Consolas;
}
/******************************  LoginForm  ******************************/
QFrame#loginbackground
{
		background:qlineargradient(spread:pad,x1:0,y1:0,x2:1,y2:1,stop:0 rgba(13,68,116,255),stop:1 rgba(30,76,79,255));
		border: 1px solid rgb(30, 81, 85);
		border-radius: 1px;
		color: rgb(255, 255, 255);
}

QWidget#logintitle
{
		border-radius: 1px;
}

QLabel#logincaption
{
		padding: 10px;
		color: rgb(255, 255, 255);
		font-size: 18px;
		font-family: 宋体;
}

QLabel#logopicture
{
		padding: 0px;
		margin: 0px;
		border: 1px inset rgba(30, 81, 85, 0);
}

QWidget#loadcontent > QProgressBar
{
		border: 0px;
		background:transparent;
		padding: 2px;
		max-height: 3px;
		color:rgb(0, 0, 0);
}

QWidget#loadcontent > QProgressBar:chunk
{
		background: qlineargradient(spread:pad,x1:0,y1:0,x2:1,y2:0,stop:0 rgba(216,216,216,255),stop:1 rgba(253,127,11,255));
}

QFrame#checkincontent
{
		background: rgba(41, 51, 64, 1);
		border: 0px inset rgb(30, 81, 85);
}

QFrame#checkincontent > QLineEdit
{
		max-width: 160px;
		padding-left: 6px;
		font-size: 20px;
		background: rgba(22, 32, 46, 1);
		color: rgba(235, 235, 235);
		border: 1px inset rgb(122, 122, 122);
}
QFrame#checkincontent > QLineEdit:hover
{
		border: 1px inset rgb(0, 120, 255);
}
QFrame#checkincontent > QLineEdit:focus
{
		border: 1px inset rgb(0, 120, 215);
}

QFrame#checkincontent > QLabel
{
		font-size: 16px;
		font-family: 黑体;
		font-weight: bold;
		color: rgb(255, 255, 255);
}

/******************************  SystemForm  ******************************/
QFrame#logcontent
{
		border: 0px inset rgb(192, 192, 192);
		background: rgb(41,51,64);
}

QFrame#systembackground,
QFrame#productbackground,
QFrame#logbackground,
QFrame#producebackground,
QFrame#versionbackground
{
		background:qlineargradient(spread:pad,x1:0,y1:0,x2:1,y2:1,stop:0 rgba(44,119,125,255),stop:1 rgba(30,76,79,255));
		border: 2px solid rgb(30, 81, 85);
		border-radius: 1px;
		color: rgb(255, 255, 255);
}

QWidget#systemtitle,
QWidget#productmtitle,
QWidget#logtitle,
QWidget#producemtitle,
QWidget#versiontitle
{
		border-radius: 1px;
}

QLabel#systemcaption,
QLabel#productcaption,
QLabel#logcaption,
QLabel#producecaption,
QLabel#versioncaption
{
		padding: 10px;
		color: rgb(255, 255, 255);
		font-size: 18px;
		font-family: 宋体;
}

QFrame#producecontent
{
		border: 0px inset rgb(192, 192, 192);
		background: rgb(41,51,64);
}

QFrame#producecontent > QComboBox
{
		height: 20px;
}

QFrame#producecontent > QLabel
{
		color: white;
		min-width:50px;
}

QFrame#producecontent > QDateTimeEdit
{
		font-size: 12px;
}

QFrame#producecontent > QTableWidget
{
		background: transparent;
		color:white;
		font-size: 10px;
}

QTableWidget#defectdetailwidget
{
		background: rgb(41, 51, 64);
		color:white;
		font-size: 10px;
}

QTableWidgetItem
{
		background: transparent;
		color:white;
		selection-background-color:lightblue;
}

QFrame#producecontent > QPushButton
{
		min-width: 86px;
}

/******************************  NavigationBar  ******************************/
QWidget[property="navigationbar"]
{
		background: rgba(236, 236, 236, 255);
}

QWidget[property="navigationbar"] > QPushButton
{
		border: none;
		border-radius: 2px;
		text-align: left;
		background: transparent;
		padding-left: 38px;
		min-height: 26px;
		font-size: 14px;
		margin-bottom: 1px;
}

QWidget[property="navigationbar"] > QPushButton:hover
{
		font-weight: bold;
		color: rgba(100,160,220,255);
		background: rgba(255, 255, 255, 255);
}

QWidget[property="navigationbar"] > QPushButton:checked
{
		font-weight: bold;
		background: rgba(255, 255, 255, 255);
		color: rgba(44,119,125,255);
}

QWidget[property="navigationbar"] > QLabel
{
		border-bottom: 3px inset rgb(216, 216, 216);
		background: transparent;
		min-height: 20px;
		font-size: 16px;
		font-weight: bold;
		padding: 10px 0px;
		margin: 1px 0px;
		color: rgb(192,192,192);
}

/******************************  PropertyContext  ******************************/
QFrame[property="context"]
{
	background: rgb(239,239,239);
    padding: 10px;
    border-radius: 1px;
}

QFrame[property="contextexpand"]
{
		background: transparent;
		padding: 10px;
		border: 0px;
}

QFrame[property="context"] > QLabel
{
    font-size: 18px;
    font-weight: bold;
    color: rgba(44,119,125,255);
    text-align: center;
}

QFrame[property="context"] > QPushButton[property="btnback"]
{
		background: transparent;
		image: url(./styles/Image/backbutton.png) center center no-repeat;
		border: 0px;
		min-height: 24px;
		max-height: 24px;
		min-width: 24px;
		max-width: 24px;
		border-radius: 1px;
}

QFrame[property="context"] > QPushButton[property="btnapply"]
{
		background: transparent;
		image: url(./styles/Image/applybutton.png) center center no-repeat;
		border: 0px;
		min-height: 24px;
		max-height: 24px;
		min-width: 24px;
		max-width: 24px;
		border-radius: 1px;
}

QFrame[property="context"] > QPushButton:hover
{
		background: transparent;
}
/******************************  PropertyColumn  ******************************/

QFrame[property="column"]
{
		padding: 10px, 0px;
}

QFrame[property="column"] > QLabel
{
	font-size: 16px;
    font-weight: bold;
}

/******************************  PropertyItem  ******************************/
QFrame[property="item"],
QFrame[property="button"],
QFrame[property="combobox"],
QFrame[property="delete"],
QFrame[property="edit"],
QFrame[property="productedit"],
QFrame[property="menu"],
QFrame[property="slider"],
QFrame[property="subcontext"],
QFrame[property="switch"]
{
		border: 1px outset rgb(177, 202, 228);
		border-radius: 1px;
		background: rgba(182, 225, 229, 255);
		padding: 10px;
		min-height: 30px;
		max-height: 60px;
}

QLabel[property="prompt"]
{
		color: rgb(166, 166, 166);
		padding: 0px;
		margin-left: 30px;
		font-size: 12px;
}


QFrame[property="item"] > QLabel[property="prompt"]
{
		color: rgb(166, 166, 166);
		font-weight: bold;
		padding: 0px;
		margin-left: 60px;
		font-size: 12px;
}

/******************************  Propertytable  ******************************/

QFrame[property="table"]
{
		border: 1px outset rgb(177, 202, 228);
		border-radius: 1px;
		background: rgba(182, 225, 229, 255);
		padding: 0px, 10px;
		min-height: 208px;
}

QFrame[property="tableproduct"]
{
		border: 1px outset rgba(177, 202, 228, 0);
		border-radius: 1px;
		background: rgba(182, 225, 229, 255);
		padding: 0px, 10px;
		min-height: 612px;
}

QFrame[property="table"] > QScrollArea,
QFrame[property="tableproduct"] > QScrollArea
{
		border: 0px;
		border-radius: 0px;
		background: rgba(182, 225, 229, 255);
}

QWidget[property="areacontext"]
{
		background: rgba(213, 229, 229, 255);
}

QFrame[property="tablebutton"]
{
		border: 0px;
		border-radius: 0px;
		border-bottom: 1px solid rgb(157, 182, 208);
		background: transparent;
		padding-left: 40px;
		min-height: 42px;
		max-height: 48px;
}

QFrame[property="tablebutton"] > QLabel
{
		color: rgba(30,76,79,255);
		font-weight: bold;
}

QFrame[property="tablebutton"] > QPushButton
{
		border: 1px inset rgba(225, 243, 244, 255);
		min-height: 18px;
		max-height: 18px;
		min-width: 20px;
		max-width: 20px;
		border-radius: 1px;
		background: rgba(239, 239, 239, 255);
		color: rgba(13,68,116,255);
		font-size: 28px;
		margin-right: 10px;
}

QFrame[property="tablebutton"] > QPushButton:hover
{
		background: rgba(44,119,125,255);
		color: rgba(239, 239, 239, 255);
}

QFrame[property="tablemenu"],
QFrame[property="expand"]
{
		border: 0px;
		border-radius: 0px;
		border-bottom: 1px solid rgb(157, 182, 208);
		padding-left: 40px;
		background: rgba(182, 225, 229, 255);
		min-height: 40px;
}

QFrame[property="expandon"]
{
		border: 0px;
		border-radius: 0px;
		border-bottom: 1px solid rgb(157, 182, 208);
		padding-left: 40px;
		background: rgba(232, 245, 255, 255);
		min-height: 40px;
}

QFrame[property="tablemenu"]:hover,
QFrame[property="expand"]:hover
{
		background: rgba(232, 245, 255, 255);
}

QFrame[property="expandon"]:hover
{
		background: rgba(255, 255, 239, 255);
}

QFrame[property="tablemenu"] > QPushButton,
QFrame[property="expand"] > QPushButton,
QFrame[property="expandon"] > QPushButton
{
		background: transparent;
		image: url(./styles/Image/menubutton.png) center center no-repeat;
		border: 0px;
		min-height: 20px;
		max-height: 20px;
		min-width: 20px;
		max-width: 20px;
		border-radius: 1px;
		font-size: 28px;
		margin-right: 10px;
}

QFrame[property="tablemenu"] > QPushButton:hover,
QFrame[property="expand"] > QPushButton:hover,
QFrame[property="expandon"] > QPushButton:hover
{
		border: 1px outset rgb(239, 239, 239);
		background: rgba(182, 225, 229, 255);
		image: url(./styles/Image/menubutton.png) center center no-repeat;
}

QFrame[property="tablemenu"] > QLabel,
QFrame[property="expand"] > QLabel,
QFrame[property="expandon"] > QLabel
{
		color: rgb(0, 0, 0);
}

QFrame[property="edit"] > QLineEdit
{
		border: 1px inset rgb(111, 156, 207);
		border-radius: 3px;
		background: rgba(41, 51, 64, 0);
		padding: 0px 6px;
		font-size: 18px;
		font-weight: bold;
		color: white;
		min-width: 100px;
}

QFrame[property="find"] > QLineEdit
{
		border: 1px inset rgb(111, 156, 207);
		border-radius: 3px;
		background: rgba(41, 51, 64, 0);
		padding: 0px 6px;
		font-size: 18px;
		font-weight: bold;
		color: rgba(235, 235, 235);
		min-width: 200px;
		min-height: 26px;
}

QFrame[property="productedit"] > QLineEdit
{
		border: 1px inset rgb(111, 156, 207);
		border-radius: 3px;
		background: rgba(225, 243, 244, 0);
		padding: 0px 6px;
		font-size: 18px;
		font-weight: bold;
		color: rgba(235, 235, 235);
		min-width: 300px;
		min-height: 26px;
}

QFrame[property="edit"] > QLineEdit:hover,
QFrame[property="productedit"] > QLineEdit:hover,
QFrame[property="find"] > QLineEdit:hover
{
		background: rgb(255, 255, 255);
		border-bottom: 1px outset rgb(255, 128, 0);
		color: rgba(235, 235, 235);
}

QFrame[property="edit"] > QLineEdit:focus,
QFrame[property="productedit"] > QLineEdit:focus
{
		color: rgb(200, 65, 132);
		background: rgb(255, 255, 255);
		border-bottom: 1px outset rgb(255, 128, 0);
		font-weight: bold;
}

QFrame[property="edit"] > QLineEdit:!enabled,
QFrame[property="productedit"] > QLineEdit:!enabled
{
		background: rgb(224,224,224);
}

QFrame[property="subcontext"] > QPushButton
{
		background: transparent;
		image: url(./styles/Image/arrowright.png) center center no-repeat;
		border: 0px;
		min-height: 12px;
		max-height: 12px;
		min-width: 12px;
		max-width: 12px;
		border-radius: 1px;
}

QFrame[property="subcontext"] > QPushButton:hover
{
		background: transparent;
}

QFrame[property="combobox"] > QComboBox
{
		min-width: 200px;
}

QFrame[property="find"] > QLabel
{
		font-size: 16px;
		font-weight: bold;
}

QFrame[property="find"] > QPushButton
{
		border: 2px inset rgba(239, 239, 239, 255);
		min-height: 20px;
		max-height: 20px;
		min-width: 20px;
		max-width: 20px;
		border-radius: 15px;
		background: rgba(44,119,125,255);
		margin-right: 10px;
		image: url(./styles/Image/find.png) center center no-repeat;
}

QFrame[property="find"] > QPushButton:hover
{
		border-style: solid;
		background: rgba(255, 255, 219, 255);
		image: url(./styles/Image/findhover.png) center center no-repeat;
		color: rgba(44,119,125,255);
}

QFrame[property="find"] > QPushButton:pressed
{
		background: rgb(0,78,161);
		border-style: outset;
		image: url(./styles/Image/find.png) center center no-repeat;
}

QFrame[property="slider"] > QSlider
{
		min-width: 200px;
}
